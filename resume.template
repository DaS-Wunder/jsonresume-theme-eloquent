        {{#if resume.basics.summary}}
          <div class="detail" id="about">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-user"></i> About</h3>

            <div class="row">
              <div class="col-xs-12">
                <p itemprop="description">{{nl2br resume.basics.summary}}</p>
              </div>
            </div>
          </div>
        {{/if}}
      </div>
    </section> -->

    <section class="col-md-9 card-wrapper pull-right">
      <div class="card">

        {{#if resume.basics.summary}}
          <div class="detail" id="about">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-user"></i> About</h3>

            <div class="row">
              <div class="col-xs-12">
                <p itemprop="description">{{nl2br resume.basics.summary}}</p>
              </div>
            </div>
          </div>
        {{/if}}

        {{#if resume.work}}
          <div class="detail" id="work-experience">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-building"></i> Work Experience</h3>

            <ul class="list-unstyled">
              {{#each resume.work}}
                <li class="detail-item row">
                  <div class="col-sm-9 has-sidebar">
                    <p class="clear-margin-sm">
                      <strong>{{position}}</strong>,&nbsp;{{company}}
                    </p>
                    <p class="text-muted visible-xs visible-sm">
                      <small>
                        <span class="space-right">
                          <time datetime="{{startDate}}" itemprop="TODO">{{formatMMMMYYYY startDate}}</time> -
                          {{#if endDate}}
                            <time datetime="{{endDate}}" itemprop="TODO">{{formatMMMMYYYY endDate}}</time>
                          {{else}}
                            <time itemprop="TODO">Present</time>
                          {{/if}}
                        </span>
                        <span>
                          <i class="fa fa-clock-o icon-left"></i>
                          {{humaneDuration startDate endDate}}
                        </span>
                      </small>
                    </p>
                    <p>{{nl2br summary}}</p>
                    {{#if highlights}}
                      <ul>
                        {{#each highlights}}
                          <li> {{.}} </li>
                        {{/each}}
                      </ul>
                    {{/if}}
                  </div>

                  <div class="col-sm-3 sidebar text-muted text-center hidden-xs hidden-sm">
                    {{#if startDate}}
                      <p>
                        <strong>Joined:</strong>&nbsp;
                        <time datetime="{{startDate}}" itemprop="TODO">{{formatMMMMYYYY startDate}}</time>
                      </p>

                      <p>
                        {{#if endDate}}
                          <strong>Left:</strong>&nbsp;
                          <time datetime="{{endDate}}" itemprop="TODO">{{formatMMMMYYYY endDate}}</time>
                        {{else}}
                          <span class="label label-success">
                            <time itemprop="TODO">Currently Working</time>
                          </span>
                        {{/if}}
                      </p>
                    {{/if}}
                    <p>
                      <i class="fa fa-clock-o icon-left"></i>
                      {{humaneDuration startDate endDate}}
                    </p>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.skills}}
          <div class="detail" id="skills">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-code"></i> Skills</h3>

            <ul class="list-unstyled">
              {{#each resume.skills}}
                <li class="detail-item row card-skills">
                  <div class="skill-info col-xs-12">
                    <strong>{{name}}</strong>

                    <div class="space-top labels">
                      {{#keywords}}
                        <span class="label label-keyword">{{.}}</span>
                      {{/keywords}}
                    </div>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.education}}
          <div class="detail" id="education">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-mortar-board"></i> Education</h3>

            <ul class="list-unstyled">
              {{#each resume.education}}
                <li class="detail-item row">
                  <div class="col-sm-9 has-sidebar">
                    <p class="clear-margin-sm">
                      <strong>{{area}}, {{studyType}}</strong>,&nbsp;
                      {{institution}}
                    </p>
                    <p class="visible-xs visible-sm text-muted">
                      <small>
                        <time datetime="{{startDate}}" itemprop="TODO">{{formatYYYY startDate}}</time> -
                        {{#if endDate}}
                          <time datetime="{{endDate}}" itemprop="TODO">{{formatYYYY endDate}}</time>
                        {{else}}
                          <time itemprop="TODO">Present</time>
                        {{/if}}
                      </small>
                    </p>
                    <i>{{gpa}}</i>
                    <div class="space-top labels">
                      {{#courses}}
                        <span class="label label-keyword">{{.}}</span>
                      {{/courses}}
                    </div>
                  </div>
                  <div class="col-sm-3 sidebar text-muted text-center hidden-xs hidden-sm">
                    <p>
                      <strong>Started:</strong>&nbsp;
                      <time datetime="{{startDate}}" itemprop="TODO">{{formatYYYY startDate}}</time>
                    </p>
                    <p>
                      {{#if endDate}}
                        <strong>Completion:</strong>&nbsp;
                        <time datetime="{{endDate}}" itemprop="TODO">{{formatYYYY endDate}}</time>
                      {{else}}
                        <span class="label label-success">
                          <time itemprop="TODO">Currently Pursuing</time>
                        </span>
                      {{/if}}
                    </p>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.awards}}
          <div class="detail" id="awards">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-trophy"></i> Awards</h3>

            <ul class="list-unstyled">
              {{#each resume.awards}}
                <li class="detail-item row">
                  <div class="col-sm-9 has-sidebar">
                    <p class="clear-margin-sm" itemprop="award">
                      <strong>{{title}}</strong>,&nbsp;
                      {{awarder}}
                    </p>
                    <p class="visible-xs visible-sm text-muted">
                      <small>
                        Awarded on: <time datetime="{{date}}" itemprop="TODO">{{formatMMMDDYYYY date}}</time>
                      </small>
                    </p>
                    <p class="clear-margin"> {{nl2br summary}} </p>
                  </div>
                  <div class="col-sm-3 sidebar text-muted text-center hidden-xs hidden-sm">
                    <p>
                      <strong>Awarded on:</strong>&nbsp;
                      <time datetime="{{date}}" itemprop="TODO">{{formatMMMDDYYYY date}}</time>
                    </p>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.volunteer}}
          <div class="detail" id="volunteer-work">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-child"></i> Volunteer Work</h3>

            <ul class="list-unstyled">
              {{#each resume.volunteer}}
                <li class="detail-item row">
                  <div class="col-sm-9 has-sidebar">
                    <p class="clear-margin-sm">
                      <strong>{{position}}</strong>,&nbsp;
                      {{organization}}
                    </p>
                    <p class="visible-xs visible-sm text-muted">
                      <small>
                        <time datetime="{{startDate}}" itemprop="TODO">{{formatMMMMYYYY startDate}}</time> -
                        {{#if endDate}}
                          <time datetime="{{endDate}}" itemprop="TODO">{{formatMMMMYYYY endDate}}</time>
                        {{else}}
                          <time itemprop="TODO">Present</time>
                        {{/if}}
                      </small>
                    </p>
                    <p>{{nl2br summary}}</p>
                    <ul>
                      {{#each highlights}}
                        <li> {{.}} </li>
                      {{/each}}
                    </ul>
                  </div>
                  <div class="col-sm-3 sidebar hidden-xs hidden-sm text-muted text-center">
                    <p>
                      <strong>Since:</strong>&nbsp;
                      <time datetime="{{startDate}}" itemprop="TODO">{{formatMMMMYYYY startDate}}</time>
                    </p>
                    <p>
                      {{#if endDate}}
                        <strong>Until:</strong>&nbsp;
                        <time datetime="{{endDate}}" itemprop="TODO">{{formatMMMMYYYY endDate}}</time>
                      {{else}}
                        <span class="label label-success">
                          Currently Volunteering
                        </span>
                      {{/if}}
                    </p>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.publications}}
          <div class="detail" id="publications">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-book"></i> Publications</h3>

            <ul class="list-unstyled">
              {{#each resume.publications}}
                <li class="detail-item row">
                  <div class="col-sm-9 has-sidebar">
                    <p class="clear-margin-sm">
                      <strong>
                        {{#if website}}
                          <a href="{{website}}"  target="_blank">{{name}}</a>
                        {{else}}
                          {{name}}
                        {{/if}}
                      </strong>,&nbsp;
                      {{publisher}}
                    </p>
                    <p class="visible-xs visible-sm text-muted">
                      <small>
                        Published on: <time datetime="{{releaseDate}}" itemprop="TODO">{{formatMMMDDYYYY releaseDate}}</time>
                      </small>
                    </p>
                    <p class="clear-margin">{{nl2br summary}}</p>
                  </div>
                  <div class="col-sm-3 sidebar text-muted text-center hidden-sm hidden-xs">
                    <p>
                      <strong>Published on:</strong>&nbsp;
                      <time datetime="{{releaseDate}}" itemprop="TODO">{{formatMMMDDYYYY releaseDate}}</time>
                    </p>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.interests}}
          <div class="detail" id="interests">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-heart"></i> Interests</h3>

            <ul class="list-unstyled">
              {{#each resume.interests}}
                <li class="detail-item row">
                  <div class="col-xs-12">
                    <p><strong>{{name}}</strong></p>

                    <div class="space-top labels">
                      {{#keywords}}
                        <span class="label label-keyword">{{.}}</span>
                      {{/keywords}}
                    </div>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}

        {{#if resume.references}}
          <div class="detail" id="references">
            <h3 class="title text-uppercase text-muted"><i class="fa fa-lg fa-thumbs-up"></i> References</h3>

            <ul class="list-unstyled">
              {{#each resume.references}}
                <li class="detail-item row">
                  <div class="col-xs-12">
                    <blockquote>
                      <p>{{reference}}</p>
                      <footer><cite title="{{name}}">{{name}}</cite></footer>
                    </blockquote>
                  </div>
                </li>
              {{/each}}
            </ul>
          </div>
        {{/if}}
      </div>
    </section>
  </div>

  <a href="#" data-scroll>Go to top</a>
  <script>
    {{> domReady}}
    {{> FastClick}}
    {{> smoothScroll}}
    {{> script}}
  </script>
</body>
</html>
